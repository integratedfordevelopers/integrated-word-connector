<?xml version="1.0" encoding="UTF-8" ?>


<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="integrated_wordconnector_index" path="/wordconnector/" methods="GET">
        <default key="_controller">IntegratedWordConnectorBundle:WordConnector:index</default>
    </route>
    <route id="integrated_wordconnector_new" path="/new" methods="POST">
        <default key="_controller">IntegratedWordConnectorBundle:WordConnector:new</default>
    </route>
</routes>